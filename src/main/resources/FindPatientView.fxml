<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.lab2.controller.FindPatientViewController" spacing="10">
  <TextField fx:id="lastNameField" promptText="Last Name"/>
  <TextField fx:id="firstNameField" promptText="First Name"/>
  <TextField fx:id="middleNameField" promptText="Middle Name"/>
  <TextField fx:id="cityNameField" promptText="City"/>
  <Button text="Find Patient" onAction="#handleFindPatient"/>
</VBox>
